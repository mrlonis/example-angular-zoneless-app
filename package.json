{
  "name": "example-angular-zoneless-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- --fix",
    "ng": "ng",
    "prepare": "husky",
    "prettier": "prettier --config .prettierrc --write \"**/*.{css,html,js,jsx,json,md,scss,ts,tsx,yaml,yml}\"",
    "prettier:test": "prettier --config .prettierrc --list-different \"**/*.{css,html,js,jsx,json,md,scss,ts,tsx,yaml,yml}\"",
    "start": "ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "lint-staged": {
    "*.{css,html,js,jsx,json,md,scss,ts,tsx,yaml,yml}": [
      "prettier --write"
    ],
    "src/**/*.{html,js,jsx,ts,tsx}": [
      "ng-lint-staged lint --fix --"
    ]
  },
  "dependencies": {
    "@angular/animations": "^19",
    "@angular/cdk": "^19",
    "@angular/common": "^19",
    "@angular/compiler": "^19",
    "@angular/core": "^19",
    "@angular/forms": "^19",
    "@angular/material": "^19",
    "@angular/platform-browser": "^19",
    "@angular/platform-browser-dynamic": "^19",
    "@angular/router": "^19",
    "rxjs": "^7",
    "tslib": "^2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19",
    "@angular/cli": "^19",
    "@angular/compiler-cli": "^19",
    "@types/jasmine": "^5",
    "angular-eslint": "^19",
    "eslint": "^9",
    "eslint-config-prettier": "^9",
    "eslint-import-resolver-typescript": "^3",
    "eslint-plugin-cypress": "^4",
    "eslint-plugin-depend": "~0.12.0",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jasmine": "^4.2.2",
    "eslint-plugin-prettier": "^5",
    "husky": "^9",
    "jasmine-core": "^5",
    "karma": "^6",
    "karma-chrome-launcher": "^3",
    "karma-coverage": "^2",
    "karma-jasmine": "^5",
    "karma-jasmine-html-reporter": "^2",
    "lint-staged": "^15",
    "ng-lint-staged": "^12",
    "prettier": "^3",
    "typescript": "~5.6",
    "typescript-eslint": "^8"
  },
  "engines": {
    "node": "^22",
    "npm": "^10"
  }
}
